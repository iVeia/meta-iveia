/dts-v1/;
/plugin/;

#include <dt-bindings/input/input.h>
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/pinctrl/pinctrl-zynqmp.h>
#include <dt-bindings/phy/phy.h>

&gem0 {
    status = "okay";
    phy-handle = <&phy0>;
    phy-mode = "sgmii";
};

&mdio0 {
    phy0: ethernet-phy@1 {
        reg = <1>;
        device_type = "ethernet-phy";
    };
};

&gem1 {
    status = "disabled";
};

// enable carrier rtc
&i2c0 {
    status = "okay";
    clock-frequency = <400000>;

    switch@77 {
        compatible = "nxp,pca9547";
        #address-cells = <1>;
        #size-cells = <0>;
        reg = <0x77>;

        i2c@0 {
            #address-cells = <1>;
            #size-cells = <0>;
            reg = <0>;

            rtc@68 {
                compatible = "st,m41t62";
                reg = <0x68>;
            };
        };
    };
};

